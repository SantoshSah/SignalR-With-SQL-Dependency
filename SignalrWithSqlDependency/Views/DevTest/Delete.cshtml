<div class="container">

    @model SignalrWithSqlDependency.Models.DevTestModel
    @using (Html.BeginForm("Delete", "DevTest"))
    {

        <div class="form-group">
            <div class="alert alert-danger">
                <strong>Are you sure want to delete this record ?</strong>
            </div>
            
        </div>
        <div class="form-group">
            @Html.HiddenFor(a=>a.ID)
            <label>Compaign Name:</label>
            @*<input type="text" class="form-control" id="compaignName">*@
            @Html.TextBoxFor(a => a.CampaignName, new { @class = "form-control",@readonly="readonly" })
        </div>
        <div class="form-group">
            <label for="date">Date:</label>
            @Html.TextBoxFor(a => a.Date, new { @class = "form-control" ,@readonly = "readonly" })
        </div>
        <div class="form-group">
            <label for="clicks">Clicks:</label>
            @Html.TextBoxFor(a => a.Clicks, new { @class = "form-control", @readonly = "readonly" })
        </div>

        <div class="form-group">
            <label for="conversions">Conversions:</label>
            @Html.TextBoxFor(a => a.Conversions, new { @class = "form-control", @readonly = "readonly" })
        </div>

        <div class="form-group">
            <label for="conversions">Impressions:</label>
            @Html.TextBoxFor(a => a.Impressions, new { @class = "form-control", @readonly = "readonly" })
        </div>
        <div class="form-group">
            <label for="conversions">AffiliateName:</label>
            @Html.TextBoxFor(a => a.AffiliateName, new { @class = "form-control", @readonly = "readonly" })
        </div>
        <button type="submit" class="btn btn-default">Delete</button>

    }

</div>